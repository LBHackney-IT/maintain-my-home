= simple_form_for @form, url: request.fullpath do |f|
  %fieldset
    %legend.question
      %h1 Is there anything else we should know?

      %p
        For example:

      %ul
        %li how does it impact your household?

    .answers
      = f.input :description, as: :text
      = f.button :submit, class: 'button'

= back_link
